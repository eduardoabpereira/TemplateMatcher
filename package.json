{
  "name": "@edumolki/template-matcher",
  "version": "2.0.22",
  "main": "dist/index",
  "typings": "dist/index",
  "description": "Locate images on your screen using template matching in nut.js",
  "author": {
    "name": "Eduardo Pereira",
    "email": "edu.eabp@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardoabpereira/TemplateMatcher.git"
  },
  "bugs": {
    "url": "https://github.com/eduardoabpereira/TemplateMatcher/issues"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "native",
    "system",
    "automation",
    "testing",
    "screen",
    "monitor",
    "display",
    "opencv"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "compile": "npm run clean && tsc -p .",
    "test": "jest --runInBand",
    "publish-next": "npm publish --tag next",
    "prepublishOnly": "npm run compile",
    "electron-rebuild": "build-opencv --electron --version 4.6.0 build"
  },
  "dependencies": {
    "@edumolki/opencv4nodejs": "^1.0.7"
  },
  "devDependencies": {
    "@nut-tree/nut-js": "^3.1.2",
    "@types/jest": "29.2.6",
    "@types/node": "^18.19.4",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "sneer": "^1.0.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@nut-tree/nut-js": ">=3.0.0"
  },
  "opencv4nodejs": {
    "autoBuildWithoutContrib": 1,
    "autoBuildOpencvVersion": "4.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
